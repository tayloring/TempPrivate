<?xml version="1.0" encoding="utf-8"?>
<com.whu.Gongyinchao.schoolservice.common.uikit.SSLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.whu.Gongyinchao.schoolservice.common.uikit.SSLinearLayout
        android:layout_weight="2"
        android:orientation="vertical"
        android:gravity="center"
        android:background="#CDDFCE"
        android:layout_margin="2dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/find_icon"
            android:paddingBottom="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/fing_geo"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </com.whu.Gongyinchao.schoolservice.common.uikit.SSLinearLayout>


    <com.whu.Gongyinchao.schoolservice.common.uikit.SSLinearLayout
        android:layout_weight="1"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.whu.Gongyinchao.schoolservice.common.uikit.SSLinearLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/geo1"
                android:layout_weight="1"
                android:layout_margin="2dp"
                android:background="#CDDFCE"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TextView
                android:id="@+id/geo2"
                android:layout_weight="1"
                android:layout_margin="2dp"
                android:background="#CDDFCE"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </com.whu.Gongyinchao.schoolservice.common.uikit.SSLinearLayout>

        <com.whu.Gongyinchao.schoolservice.common.uikit.SSLinearLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/geo3"
                android:layout_weight="1"
                android:background="#CDDFCE"
                android:gravity="center"
                android:layout_margin="2dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TextView
                android:id="@+id/geo4"
                android:layout_weight="1"
                android:background="#CDDFCE"
                android:layout_margin="2dp"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </com.whu.Gongyinchao.schoolservice.common.uikit.SSLinearLayout>
    </com.whu.Gongyinchao.schoolservice.common.uikit.SSLinearLayout>
</com.whu.Gongyinchao.schoolservice.common.uikit.SSLinearLayout>